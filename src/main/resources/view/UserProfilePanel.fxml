<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane id="userProfilePane" fx:id="userProfilePane" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">

    <stylesheets>
        <URL value="@UserProfilePanel.css" />
    </stylesheets>

    <VBox alignment="TOP_LEFT" minHeight="105">
        <padding>
            <Insets bottom="10" left="10" right="10" top="10" />
        </padding>
        <Label fx:id="name" text="\$name" wrapText="true"/>
        <Label fx:id="phone" text="\$phone" wrapText="true"/>
        <Label fx:id="email" text="\$email" wrapText="true"/>
        <Label fx:id="address" text="\$address" wrapText="true"/>
        <Label fx:id="telegramHandle" text="\$telegramHandle" wrapText="true"/>
        <Label fx:id="moduleTags" text="\$moduleTags" wrapText="true" />
        <Label fx:id="groupTags" text="\$groupTags" wrapText="true" />
    </VBox>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <StackPane.margin>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </StackPane.margin>
</StackPane>
