<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane id="userProfilePane" fx:id="userProfilePane" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">

    <stylesheets>
        <URL value="@UserProfilePanel.css" />
    </stylesheets>

    <VBox alignment="TOP_LEFT">
        <Label fx:id="name" text="\$name" />
        <Label fx:id="phone" text="\$phone" />
        <Label fx:id="email" text="\$email" />
        <Label fx:id="address" text="\$address" />
        <Label fx:id="telegramHandle" text="\$telegramHandle" />
        <Label fx:id="moduleTags" text="\$moduleTags" />
        <Label fx:id="groupTags" text="\$groupTags" wrapText="true" />
    </VBox>
    <padding>
        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
    </padding>
    <StackPane.margin>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </StackPane.margin>
</StackPane>
